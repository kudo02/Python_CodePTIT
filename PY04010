class ThiSinh:
    def __init__(self, name, date, d1, d2, d3):
        self.name = name
        self.date = date
        self.d1 = d1
        self.d2 = d2
        self.d3 = d3
    
    def out(self):
        print(name, date, "{:.1f}".format(d1 + d2 + d3), sep=" ")

name = input()
date = input()
d1 = float(input())
d2 = float(input())
d3 = float(input())
r = ThiSinh(name, date, d1, d2, d3)
r.out()

